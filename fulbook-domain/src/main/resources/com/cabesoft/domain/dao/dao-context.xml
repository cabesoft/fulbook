<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="physicalItemDao" class="com.cabesoft.domain.dao.impl.PhysicalItemDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean id="socialItemDao" class="com.cabesoft.domain.dao.impl.SocialItemDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean id="teamItemDao" class="com.cabesoft.domain.dao.impl.TeamItemDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean id="physicalStatDao" class="com.cabesoft.domain.dao.impl.PhysicalStatDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean id="socialStatDao" class="com.cabesoft.domain.dao.impl.SocialStatDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean id="teamStatDao" class="com.cabesoft.domain.dao.impl.TeamStatDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean id="playerDao" class="com.cabesoft.domain.dao.impl.PlayerDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>

</beans>